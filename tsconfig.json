{
	"compilerOptions": {
		"lib": ["ESNext"],
		"module": "nodenext",
		"moduleResolution": "nodenext",
		"target": "ES2022",
		"esModuleInterop": true,
		"strict": true,
		"skipLibCheck": true,
		"allowSyntheticDefaultImports": true,
		"noEmit": true,
		"allowImportingTsExtensions": true,
		"noUncheckedIndexedAccess": true
	},
	"include": ["src"]
}
